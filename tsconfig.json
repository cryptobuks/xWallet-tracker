{
  "compileOnSave": false,
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "target": "es2017",
    "module": "commonjs",
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "strictNullChecks": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "inlineSourceMap": true,
    "lib": [
      "es2016",
      "es2017",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "node",
      "webpack-env"
    ]
  }
}